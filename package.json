{
  "name": "@pandell/leaflet-draw",
  "version": "1.0.4",
  "description": "Vector drawing plugin for Leaflet (Pandell fork)",
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-mourner": "^3.0.0",
    "git-rev": "^0.2.1",
    "happen": "^0.3.2",
    "jake": "^10.8.5",
    "jshint": "^2.13.5",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "~2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "~1.0.0",
    "leafdoc": "^2.3.0",
    "leaflet": "^0.7.7",
    "mocha": "^9.2.2",
    "phantomjs-prebuilt": "^2.1.16",
    "prosthetic-hand": "^1.4.0",
    "source-map": "^0.7.4",
    "uglify-js": "^2.8.29",
    "uglifycss": "0.0.29"
  },
  "main": "dist/leaflet.draw.js",
  "style": "dist/leaflet.draw.css",
  "directories": {
    "doc": "docs/",
    "example": "docs/examples/",
    "lib": "src/"
  },
  "files": [
    "dist/**/*",
    "docs/**/*"
  ],
  "scripts": {
    "build": "yarn run test && yarn run build:docs && yarn run build:dist",
    "build:dist": "jake build",
    "build:docs": "jake docs",
    "test": "jake test",
    "prepack": "yarn run build && yarn run prepack:clean-package-json",
    "prepack:clean-package-json": "node build/clean-package-json.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pandell/Leaflet.draw.git"
  },
  "keywords": [
    "maps",
    "leaflet",
    "client",
    "vector",
    "drawing",
    "draw"
  ],
  "contributors": [
    {
      "name": "Jon West",
      "email": "ddproxy@gmail.com"
    },
    {
      "name": "Jacob Toye"
    },
    {
      "name": "Pandell Technology Corporation",
      "url": "https://github.com/pandell"
    }
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "eslintConfig": {
    "root": true,
    "globals": {
      "L": true
    },
    "env": {
      "commonjs": true,
      "amd": true,
      "node": false
    },
    "extends": "mourner",
    "rules": {
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "indent": [
        2,
        "tab",
        {
          "VariableDeclarator": 0
        }
      ],
      "curly": 2,
      "spaced-comment": 2,
      "strict": 0,
      "wrap-iife": 0,
      "key-spacing": 0,
      "consistent-return": 0
    }
  }
}
